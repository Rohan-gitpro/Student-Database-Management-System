<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="600.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.deleteRecordController">
	<children>
		<AnchorPane prefHeight="64.0" prefWidth="600.0"
			style="-fx-background-color: #ef5350;">
			<children>
				<ImageView fx:id="goBack" fitHeight="31.0" fitWidth="28.0"
					layoutY="50.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@icons/icons8-go-back-24.png" />
					</image>
				</ImageView>
				<Label layoutX="175.0" layoutY="19.0" text="DELETE RECORD"
					textFill="#ebe9e9">
					<font>
						<Font name="Algerian" size="36.0" />
					</font>
				</Label>
			</children>
		</AnchorPane>
		<TextField fx:id="rollNumberField" layoutX="226.0"
			layoutY="192.0" prefHeight="26.0" prefWidth="208.0"
			promptText="ENTER ROLL NUMBER">
			<font>
				<Font name="Britannic Bold" size="14.0" />
			</font>
		</TextField>
		<Label layoutX="55.0" layoutY="194.0" text="ROLL NUMBER">
			<font>
				<Font name="Arial Rounded MT Bold" size="18.0" />
			</font>
		</Label>
		<Button fx:id="deleteRecordButton" layoutX="225.0"
			layoutY="261.0" mnemonicParsing="false" prefHeight="33.0"
			prefWidth="168.0" text="DELETE ">
			<font>
				<Font name="Bookman Old Style Bold" size="18.0" />
			</font>
		</Button>
	</children>
</AnchorPane>
