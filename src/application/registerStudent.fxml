<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="600.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.registerStudentController">
	<children>
		<AnchorPane prefHeight="75.0" prefWidth="600.0"
			style="-fx-background-color: #ef5350;">
			<children>
				<Label alignment="CENTER" layoutX="113.0" layoutY="18.0"
					prefHeight="40.0" prefWidth="374.0" text="REGISTER STUDENT"
					textFill="#eeeeee">
					<font>
						<Font name="Algerian" size="36.0" />
					</font>
				</Label>
				<ImageView fx:id="goBack" fitHeight="26.0" fitWidth="30.0"
					layoutX="14.0" layoutY="38.0" pickOnBounds="true"
					preserveRatio="true">
					<image>
						<Image url="@icons/icons8-go-back-24.png" />
					</image>
				</ImageView>
			</children>
		</AnchorPane>
		<Label layoutX="34.0" layoutY="128.0" prefHeight="29.0"
			prefWidth="120.0" text="NAME">
			<font>
				<Font name="Britannic Bold" size="18.0" />
			</font>
		</Label>
		<Label layoutX="34.0" layoutY="85.0" prefHeight="29.0"
			prefWidth="120.0" text="ROLL NUMBER">
			<font>
				<Font name="Britannic Bold" size="18.0" />
			</font>
		</Label>
		<TextField fx:id="rollNumber" layoutX="204.0" layoutY="87.0"
			prefHeight="26.0" prefWidth="228.0" promptText="ENTER ROLL NUMBER">
			<font>
				<Font name="Bookman Old Style Bold" size="14.0" />
			</font>
		</TextField>
		<TextField fx:id="name" layoutX="204.0" layoutY="130.0"
			prefHeight="26.0" prefWidth="228.0" promptText="ENTER NAME">
			<font>
				<Font name="Bookman Old Style Bold" size="14.0" />
			</font>
		</TextField>
		<Button fx:id="registerButton" layoutX="243.0" layoutY="440.0"
			mnemonicParsing="false" prefHeight="34.0" prefWidth="151.0"
			text="REGISTER">
			<font>
				<Font name="Arial Rounded MT Bold" size="18.0" />
			</font>
		</Button>
		<Label layoutX="34.0" layoutY="180.0" prefHeight="29.0"
			prefWidth="120.0" text="E-MAIL">
			<font>
				<Font name="Britannic Bold" size="18.0" />
			</font>
		</Label>
		<Label layoutX="34.0" layoutY="236.0" prefHeight="29.0"
			prefWidth="120.0" text="LOCATION">
			<font>
				<Font name="Britannic Bold" size="18.0" />
			</font>
		</Label>
		<Label layoutX="34.0" layoutY="286.0" prefHeight="29.0"
			prefWidth="120.0" text="NUMBER">
			<font>
				<Font name="Britannic Bold" size="18.0" />
			</font>
		</Label>
		<Label layoutX="34.0" layoutY="335.0" prefHeight="29.0"
			prefWidth="120.0" text="BLOOD GROUP">
			<font>
				<Font name="Britannic Bold" size="18.0" />
			</font>
		</Label>
		<Label layoutX="34.0" layoutY="381.0" prefHeight="29.0"
			prefWidth="120.0" text="GENDER">
			<font>
				<Font name="Britannic Bold" size="18.0" />
			</font>
		</Label>
		<TextField fx:id="email" layoutX="205.0" layoutY="182.0"
			prefHeight="26.0" prefWidth="228.0" promptText="ENTER E-MAIL">
			<font>
				<Font name="Bookman Old Style Bold" size="14.0" />
			</font>
		</TextField>
		<TextField fx:id="locationfield" layoutX="204.0"
			layoutY="238.0" prefHeight="26.0" prefWidth="228.0"
			promptText="ENTER LOCATION">
			<font>
				<Font name="Bookman Old Style Bold" size="14.0" />
			</font>
		</TextField>
		<TextField fx:id="number" layoutX="204.0" layoutY="288.0"
			prefHeight="26.0" prefWidth="228.0" promptText="ENTER NUMBER">
			<font>
				<Font name="Bookman Old Style Bold" size="14.0" />
			</font>
		</TextField>
		<TextField fx:id="bloddgroup" layoutX="204.0"
			layoutY="337.0" prefHeight="26.0" prefWidth="228.0"
			promptText="ENTER BLOOD GROUP">
			<font>
				<Font name="Bookman Old Style Bold" size="14.0" />
			</font>
		</TextField>
		<CheckBox fx:id="maleCheckBox" layoutX="204.0"
			layoutY="386.0" mnemonicParsing="false" prefHeight="19.0"
			prefWidth="74.0" text="MALE">
			<font>
				<Font name="Bookman Old Style Bold" size="14.0" />
			</font>
		</CheckBox>
		<CheckBox fx:id="femaleCheckBox" layoutX="337.0"
			layoutY="386.0" mnemonicParsing="false" prefHeight="19.0"
			prefWidth="92.0" text="FEMALE">
			<font>
				<Font name="Bookman Old Style Bold" size="14.0" />
			</font>
		</CheckBox>
	</children>
</AnchorPane>
