<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="600.0"
	style="-fx-background-color: #f5f5f5;"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.mainPageController">
	<children>
		<AnchorPane prefHeight="62.0" prefWidth="600.0"
			style="-fx-background-color: #f44336;">
			<children>
				<Label alignment="CENTER" layoutX="52.0" layoutY="10.0"
					prefHeight="27.0" prefWidth="496.0" text="WELCOME">
					<font>
						<Font name="Arial Rounded MT Bold" size="36.0" />
					</font>
				</Label>
			</children>
		</AnchorPane>
		<Button fx:id="enterMarksButton" layoutX="8.0" layoutY="199.0"
			mnemonicParsing="false" prefHeight="25.0" prefWidth="133.0"
			text="Enter Marks">
			<font>
				<Font name="Britannic Bold" size="14.0" />
			</font>
		</Button>
		<Button fx:id="deleteRecordButton" layoutX="157.0"
			layoutY="199.0" mnemonicParsing="false" prefHeight="25.0"
			prefWidth="133.0" text="Delete Record">
			<font>
				<Font name="Britannic Bold" size="14.0" />
			</font>
		</Button>
		<Button fx:id="updateButton" layoutX="306.0" layoutY="199.0"
			mnemonicParsing="false" prefHeight="25.0" prefWidth="133.0"
			text="Update Marks">
			<font>
				<Font name="Britannic Bold" size="14.0" />
			</font>
		</Button>
		<Button fx:id="resultButton" layoutX="459.0" layoutY="199.0"
			mnemonicParsing="false" prefHeight="25.0" prefWidth="133.0"
			text="Result">
			<font>
				<Font name="Britannic Bold" size="14.0" />
			</font>
		</Button>
		<Button fx:id="registerButton" layoutX="75.0" layoutY="262.0"
			mnemonicParsing="false" prefHeight="26.0" prefWidth="145.0"
			text="Register Student">
			<font>
				<Font name="Britannic Bold" size="14.0" />
			</font>
		</Button>
		<Button fx:id="detailButton" layoutX="367.0" layoutY="262.0"
			mnemonicParsing="false" prefHeight="26.0" prefWidth="145.0"
			text="Student Details">
			<font>
				<Font name="Britannic Bold" size="14.0" />
			</font>
		</Button>
	</children>
</AnchorPane>
