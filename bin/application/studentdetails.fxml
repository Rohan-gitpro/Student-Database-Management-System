<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="600.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.studentdetailsController">
	<children>
		<AnchorPane prefHeight="57.0" prefWidth="600.0"
			style="-fx-background-color: #ef5350;">
			<children>
				<Label layoutX="119.0" layoutY="7.0" text="STUDENT DETAILS"
					textFill="#eee7e7">
					<font>
						<Font name="Algerian" size="48.0" />
					</font>
				</Label>
				<ImageView fx:id="goBack" fitHeight="23.0" fitWidth="24.0"
					layoutX="3.0" layoutY="23.0" pickOnBounds="true"
					preserveRatio="true">
					<image>
						<Image url="@icons/icons8-go-back-24.png" />
					</image>
				</ImageView>
			</children>
		</AnchorPane>
		<Label layoutX="23.0" layoutY="75.0" prefHeight="23.0"
			prefWidth="107.0" text="ROLL NUMBER">
			<font>
				<Font name="Britannic Bold" size="16.0" />
			</font>
		</Label>
		<Label layoutX="31.0" layoutY="207.0" prefHeight="23.0"
			prefWidth="107.0" text="NAME">
			<font>
				<Font name="Britannic Bold" size="16.0" />
			</font>
		</Label>
		<Label layoutX="31.0" layoutY="256.0" prefHeight="23.0"
			prefWidth="107.0" text="GENDER">
			<font>
				<Font name="Britannic Bold" size="16.0" />
			</font>
		</Label>
		<Label layoutX="31.0" layoutY="301.0" prefHeight="23.0"
			prefWidth="107.0" text="E-MAIL">
			<font>
				<Font name="Britannic Bold" size="16.0" />
			</font>
		</Label>
		<Label layoutX="31.0" layoutY="352.0" prefHeight="23.0"
			prefWidth="107.0" text="LOCATION">
			<font>
				<Font name="Britannic Bold" size="16.0" />
			</font>
		</Label>
		<Label layoutX="31.0" layoutY="403.0" prefHeight="23.0"
			prefWidth="107.0" text="NUMBER">
			<font>
				<Font name="Britannic Bold" size="16.0" />
			</font>
		</Label>
		<Label layoutX="31.0" layoutY="450.0" prefHeight="23.0"
			prefWidth="107.0" text="BLOOD GROUP">
			<font>
				<Font name="Britannic Bold" size="16.0" />
			</font>
		</Label>
		<Button fx:id="showDetailsButton" layoutX="104.0"
			layoutY="132.0" mnemonicParsing="false" text="SHOW DETAILS">
			<font>
				<Font name="Arial Rounded MT Bold" size="16.0" />
			</font>
		</Button>
		<TextField fx:id="rollNumber" layoutX="177.0" layoutY="71.0"
			prefHeight="31.0" prefWidth="224.0" promptText="ENTER ROLL NUMBER">
			<font>
				<Font name="Bookman Old Style" size="16.0" />
			</font>
		</TextField>
		<Label fx:id="name" layoutX="177.0" layoutY="210.0"
			prefHeight="23.0" prefWidth="224.0" text="STUDENT NAME">
			<font>
				<Font name="Bookman Old Style Bold" size="16.0" />
			</font>
		</Label>
		<Label fx:id="gender" layoutX="177.0" layoutY="256.0"
			prefHeight="23.0" prefWidth="224.0" text="STUDENT GENDER">
			<font>
				<Font name="Bookman Old Style Bold" size="16.0" />
			</font>
		</Label>
		<Label fx:id="email" layoutX="177.0" layoutY="301.0"
			prefHeight="23.0" prefWidth="415.0" text="STUDENT EMAIL">
			<font>
				<Font name="Bookman Old Style Bold" size="16.0" />
			</font>
		</Label>
		<Label fx:id="locationLabel" layoutX="177.0" layoutY="352.0"
			prefHeight="23.0" prefWidth="224.0" text="STUDENT LOCATION">
			<font>
				<Font name="Bookman Old Style Bold" size="16.0" />
			</font>
		</Label>
		<Label fx:id="number" layoutX="177.0" layoutY="403.0"
			prefHeight="23.0" prefWidth="224.0" text="STUDENT NUMBER">
			<font>
				<Font name="Bookman Old Style Bold" size="16.0" />
			</font>
		</Label>
		<Label fx:id="bloodgroup" layoutX="177.0" layoutY="450.0"
			prefHeight="23.0" prefWidth="224.0" text="STUDENT BLOOD GROUP">
			<font>
				<Font name="Bookman Old Style Bold" size="16.0" />
			</font>
		</Label>
	</children>
</AnchorPane>
