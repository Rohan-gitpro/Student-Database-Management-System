<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.signUpController">
	<children>
		<AnchorPane prefHeight="64.0" prefWidth="700.0"
			style="-fx-background-color: #ef5350;">
			<children>
				<Label alignment="CENTER" layoutX="188.0" layoutY="12.0"
					prefHeight="40.0" prefWidth="325.0" text="SIGN-UP"
					textFill="#eee8e8">
					<font>
						<Font name="Algerian" size="36.0" />
					</font>
				</Label>
			</children>
		</AnchorPane>
		<Label layoutX="203.0" layoutY="125.0" prefHeight="29.0"
			prefWidth="115.0" text="USERNAME">
			<font>
				<Font name="Britannic Bold" size="18.0" />
			</font>
		</Label>
		<Label layoutX="203.0" layoutY="173.0" prefHeight="29.0"
			prefWidth="115.0" text="PASSWORD">
			<font>
				<Font name="Britannic Bold" size="18.0" />
			</font>
		</Label>
		<Label layoutX="203.0" layoutY="221.0" prefHeight="29.0"
			prefWidth="115.0" text="EMAIL">
			<font>
				<Font name="Britannic Bold" size="18.0" />
			</font>
		</Label>
		<Label layoutX="203.0" layoutY="275.0" prefHeight="29.0"
			prefWidth="115.0" text="GENDER">
			<font>
				<Font name="Britannic Bold" size="18.0" />
			</font>
		</Label>
		<TextField fx:id="userName" layoutX="350.0" layoutY="126.0"
			prefHeight="27.0" prefWidth="190.0" promptText="USERNAME">
			<font>
				<Font name="Arial Rounded MT Bold" size="14.0" />
			</font>
		</TextField>
		<TextField fx:id="password" layoutX="350.0" layoutY="174.0"
			prefHeight="27.0" prefWidth="190.0" promptText="PASSWORD">
			<font>
				<Font name="Arial Rounded MT Bold" size="14.0" />
			</font>
		</TextField>
		<TextField fx:id="email" layoutX="350.0" layoutY="222.0"
			prefHeight="27.0" prefWidth="190.0" promptText="EMAIL">
			<font>
				<Font name="Arial Rounded MT Bold" size="14.0" />
			</font>
		</TextField>
		<CheckBox fx:id="maleCheckbox" layoutX="361.0"
			layoutY="280.0" mnemonicParsing="false" text="MALE">
			<font>
				<Font name="Arial Rounded MT Bold" size="14.0" />
			</font>
		</CheckBox>
		<CheckBox fx:id="femaleCheckBox" layoutX="457.0"
			layoutY="280.0" mnemonicParsing="false" text="FEMALE">
			<font>
				<Font name="Arial Rounded MT Bold" size="14.0" />
			</font>
		</CheckBox>
		<Button fx:id="signUpButton" layoutX="279.0" layoutY="342.0"
			mnemonicParsing="false" prefHeight="30.0" prefWidth="153.0"
			text="SIGN UP">
			<font>
				<Font name="Britannic Bold" size="18.0" />
			</font>
		</Button>
	</children>
</AnchorPane>
